{
  "cleanUrls": true,
  "trailingSlash": false,
  "headers": [
    {
      "source": "/admin.js",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/javascript"
        }
      ]
    },
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-store, no-cache, must-revalidate, proxy-revalidate"
        }
      ]
    }
  ],
  "redirects": [
    {
      "source": "/_next/static/(.*)",
      "destination": "/",
      "statusCode": 404
    },
    {
      "source": "/_next/(.*)",
      "destination": "/",
      "statusCode": 404
    },
    {
      "source": "/api/(.*)",
      "destination": "/",
      "statusCode": 404
    },
    {
      "source": "/insights/(.*)",
      "destination": "/",
      "statusCode": 404
    },
    {
      "source": "/stream/external/(.*)",
      "destination": "/",
      "statusCode": 404
    }
  ]
}